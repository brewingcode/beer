{
  "name": "yardhouse",
  "version": "1.0.0",
  "description": "A better listing of Yard House beers",
  "author": "Alex <alex@brewingcode.net>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "generate:gh": "ROUTER_BASE=/yardhouse/ npm run generate && git checkout gh-pages && rsync -a dist/ ."
  },
  "dependencies": {
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.1",
    "cheerio": "^1.0.0-rc.2",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "pug": "2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
